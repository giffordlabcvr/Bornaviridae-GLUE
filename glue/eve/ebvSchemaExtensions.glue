
  # Create schema extensions for EVEs
  create custom-table locus_data

  table locus_data
  
    create field scaffold VARCHAR 100
    create field start INTEGER
    create field end INTEGER
    create field orientation VARCHAR 20
    create field length INTEGER
    create field assigned_name VARCHAR 100
    create field assigned_gene VARCHAR 100
    create field bitscore VARCHAR 20

    exit

  create link sequence locus_data
